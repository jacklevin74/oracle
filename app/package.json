{
  "name": "oracle-price-updater",
  "version": "2.0.0",
  "description": "TypeScript-based Oracle Price Updater for Solana blockchain",
  "main": "dist/index.js",
  "bin": {
    "oracle-updater": "./dist/index.js"
  },
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "server": "node server.mjs",
    "test:jupiter": "ts-node test-jupiter.ts",
    "test:dexscreener": "ts-node test-dexscreener.ts",
    "test:birdeye": "ts-node test-birdeye.ts",
    "test:aggregation": "ts-node test-aggregation.ts",
    "test:quality": "ts-node test-quality.ts",
    "test:registry": "ts-node test-registry.ts",
    "test:integration": "ts-node test-integration.ts",
    "test:live": "ts-node test-live.ts",
    "test:v3": "npm run test:jupiter && npm run test:aggregation && npm run test:quality && npm run test:registry",
    "test:e2e": "ts-node test-e2e.ts",
    "dashboard": "ts-node dashboard-server.ts",
    "dashboard:blockchain": "node server.mjs",
    "dashboard:relay": "node relay-dashboard.mjs",
    "controller:dry-run": "ANCHOR_PROVIDER_URL=https://rpc.mainnet.x1.xyz node dist/controller/controller-process.js --dry-run --foreground",
    "controller:dry-run-verbose": "ANCHOR_PROVIDER_URL=https://rpc.mainnet.x1.xyz node dist/controller/controller-process.js --dry-run --foreground --verbose",
    "controller:start": "ANCHOR_PROVIDER_URL=https://rpc.mainnet.x1.xyz node dist/controller/controller-process.js --prompt"
  },
  "keywords": [
    "solana",
    "oracle",
    "price-feed",
    "pyth",
    "cryptocurrency",
    "blockchain"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pythnetwork/hermes-client": "^2.0.0",
    "@pythnetwork/price-service-client": "^1.9.0",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^5.0.0",
    "express": "^5.1.0",
    "protobufjs": "^7.4.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^20.17.10",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
